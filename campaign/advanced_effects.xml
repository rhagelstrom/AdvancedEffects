    </windowclass>

    <windowclass name="item" merge="join">
        <sheetdata>
            <sub_advanced_effects name="sub_advanced_effects"  insertbefore="sub_version"/>
        </sheetdata>
    </windowclass>

    <windowclass name="reference_race" merge="join">
        <sheetdata>
            <sub_advanced_effects name="sub_advanced_effects"  insertbefore="sub_version"/>
        </sheetdata>
    </windowclass>

    <windowclass name="reference_subrace" merge="join">
        <sheetdata>
            <sub_advanced_effects name="sub_advanced_effects"  insertbefore="sub_version"/>
        </sheetdata>
    </windowclass>

    <windowclass name="npc" merge="join">
        <sheetdata>
            <sub_advanced_effects name="sub_advanced_effects"  insertbefore="sub_version"/>
        </sheetdata>
    </windowclass>

    <windowclass name="charsheet_abilities" merge="join" insertafter="languages">
